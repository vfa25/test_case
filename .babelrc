{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "7",
          "firefox": "60",
          "chrome": "50",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": 2
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}
